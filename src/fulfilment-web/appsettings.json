{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Debug",
    "Enrich": [ "WithMachineName" ]
  },
  "Observability": {
    "Trace": {
      "Jaeger": false,
      "Console": true,
      "Agent": {
        "Host": "jaeger",
        "Port": 6831
      },
      "CustomSpans": true
    }
  },
  "Documents": {
    "Api": {
      "BaseUrl": "http://localhost:8071"
    },
    "Authz": {
      "Url": "http://localhost:8072/check"
    }
  }
}
